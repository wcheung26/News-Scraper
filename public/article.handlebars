<div class="col-xs-10 col-xs-offset-1">
	<div class="page-header">
		<h1>{{title}}</h1>
		<a href={{link}}> <h4>{{link}}</h4> </a>
	</div>
	<div>
		<h3>Comments for this article</h3>
		<ul class="list-group">
			{{#each note}}
				<li class="list-group-item commentBox">
					<h4>{{this.title}} <span class="small"> {{this.username}}</span></h4>
					<p>{{this.body}}</p>
				</li>
		  	{{/each}}
		</ul>
		<p class="text-center"> <a href="/">Back to Articles</a> </p>
	</div>
</div>